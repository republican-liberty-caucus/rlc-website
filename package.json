{
  "name": "rlc-website",
  "version": "1.0.0",
  "private": true,
  "description": "Republican Liberty Caucus - Next.js website with Supabase and HighLevel integration",
  "author": "RLC",
  "license": "PROPRIETARY",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "prisma:generate": "prisma generate",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:push": "prisma db push",
    "prisma:studio": "prisma studio",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:prod": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,md,json}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,md,json}\"",
    "prepare": "husky install",
    "migrate:civicrm": "tsx --env-file=.env.local scripts/civicrm/migrate-civicrm.ts",
    "migrate:civicrm:dry-run": "tsx --env-file=.env.local scripts/civicrm/migrate-civicrm.ts --dry-run",
    "snapshot:highlevel": "tsx --env-file=.env.local scripts/civicrm/snapshot-before-highlevel.ts",
    "import:highlevel": "tsx --env-file=.env.local scripts/civicrm/import-highlevel.ts",
    "validate:highlevel": "tsx --env-file=.env.local scripts/civicrm/validate-highlevel-import.ts",
    "rollback:highlevel": "tsx --env-file=.env.local scripts/civicrm/rollback-highlevel.ts",
    "sync:highlevel": "tsx --env-file=.env.local scripts/civicrm/sync-highlevel.ts",
    "stripe:setup-products": "tsx --env-file=.env.local scripts/stripe-setup-products.ts",
    "cleanup:charters": "tsx --env-file=.env.local scripts/civicrm/cleanup-charters.ts",
    "cleanup:charters:dry-run": "tsx --env-file=.env.local scripts/civicrm/cleanup-charters.ts --dry-run",
    "migrate:wordpress": "tsx scripts/wordpress/migrate-wordpress.ts",
    "migrate:wordpress:dry-run": "tsx scripts/wordpress/migrate-wordpress.ts --dry-run",
    "migrate:wordpress:media": "tsx scripts/wordpress/migrate-media.ts",
    "migrate:wordpress:media:dry-run": "tsx scripts/wordpress/migrate-media.ts --dry-run",
    "seed:demo": "tsx --env-file=.env.local scripts/seed-demo.ts",
    "seed:demo:teardown": "tsx --env-file=.env.local scripts/seed-demo.ts --teardown",
    "demo:record": "playwright test --project=demo"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.38",
    "@clerk/nextjs": "^6.37.3",
    "@hookform/resolvers": "^3.3.4",
    "@prisma/client": "^6.19.1",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@stripe/react-stripe-js": "^5.6.0",
    "@stripe/stripe-js": "^8.7.0",
    "@supabase/supabase-js": "^2.39.0",
    "@tailwindcss/typography": "^0.5.19",
    "@tanstack/react-query": "^5.90.5",
    "@tanstack/react-table": "^8.21.3",
    "ai": "^6.0.77",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "date-fns": "^3.6.0",
    "framer-motion": "^11.18.2",
    "lucide-react": "^0.320.0",
    "next": "^15.5.12",
    "next-themes": "^0.4.6",
    "novel": "^1.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.49.0",
    "react-simple-maps": "^3.0.0",
    "resend": "^3.0.0",
    "sanitize-html": "^2.17.0",
    "stripe": "^14.25.0",
    "svix": "^1.44.0",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.4",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.1",
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-dom": "^6.4.0",
    "@testing-library/react": "^14.2.0",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/react-simple-maps": "^3.0.6",
    "@types/sanitize-html": "^2.16.0",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "@vitest/coverage-v8": "^4.0.18",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-config-next": "^15.5.12",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.0.0",
    "lint-staged": "^15.2.0",
    "mysql2": "^3.16.3",
    "postcss": "^8.4.33",
    "prettier": "^3.2.0",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "prisma": "^6.19.1",
    "tailwindcss": "^3.4.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vitest": "^4.0.18"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{md,json}": [
      "prettier --write"
    ]
  }
}
